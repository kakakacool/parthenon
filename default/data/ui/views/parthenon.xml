<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>Parthenon</label>
  <row>
    <single>
      <searchName>parthenon total submission</searchName>
      <title>Total Submissions</title>
    </single>
    <single>
      <searchName>parthenon all time distinct actors</searchName>
      <title>Bad Actors on Record</title>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </single>
    <single>
      <searchName>parthenon all time distinct md5sums</searchName>
      <title>MD5Sums on Record</title>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </single>
  </row>
  <row>
    <chart>
      <searchName>parthenon submissions by user</searchName>
      <title>Submission Rate</title>
      <option name="charting.chart">area</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </chart>
    <chart>
      <searchName>parthenon retrieval rate by user</searchName>
      <title>parthenon retrieval rate by user</title>
      <option name="charting.chart">area</option>
    </chart>
  </row>
  <row>
    <table>
      <searchName>parthenon Users Mode UNique Requests</searchName>
      <title>Retrievals</title>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </table>
    <table>
      <searchName>parthenon submissions by user host</searchName>
      <title>Submissions</title>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
</dashboard>
